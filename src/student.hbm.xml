<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name = "com.sneha.hibernate.Student" table="student">
<id name ="id">
<generator class="identity"></generator>
</id>
<property name="firstName" column="first"></property>
<property name="lastName" column="second"></property>
</class>
</hibernate-mapping>